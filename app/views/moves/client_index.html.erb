
<div class="container">
      <%= @moves.each do |move|%>
  <div class="card-header">

      <h5 class="card-title"><%= move.date %></h5>
      <p class="card-text"><%= move.start_point %></p>
      <p class="card-text"><%= move.end_point %></p>
      <p class="card-text"><%= move.shipment_info %></p>
      <p class="card-text"><%= move.status %></p>
      <p class="card-text"><%= move.expiration %></p>
      <%= link_to "Edit", edit_move_path(move.id), class: "order-1 btn btn-success text-light" %>
      <%= link_to "", move_path(move.id),  class: "order-3 btn btn-outline-danger fa-solid fa-trash-can pt-2", data: { turbo_method: :delete, turbo_confirm: "Are you sure?" } %>
  </div>
  <% end %>
</div>
